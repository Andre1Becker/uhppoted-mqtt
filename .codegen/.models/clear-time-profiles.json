{
  "clear_time_profiles": {
    "command": "clear-time-profiles",
    "request": {
      "topic": "uhppoted/gateway/requests/device/time-profiles:delete",
      "fields": [
        {
          "field": "device-id",
          "value": "uint32",
          "description": "(required) controller serial number"
        }
      ]
    },
    "response": {
      "fields": [
        {
          "field": "device-id",
          "value": "<controller-id>",
          "description": "controller serial number"
        },
        {
          "field": "deleted",
          "value": "bool",
          "description": "clear time profiles success/fail"
        }
      ]
    }
  }
}
